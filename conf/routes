# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /search                     controllers.SearchController.search
GET		/terms						controllers.SearchController.similarTerms(term: String, level: String ?= "paragraph", maxEditDistance: Int ?= 2, minCommonPrefix: Int ?= 0, transposeIsSingleEdit: Option[String])
GET		/stats						controllers.SearchController.stats(query: String, termFreqs: Option[String])
GET		/collocations				controllers.SearchController.collocations
GET		/termVectorDiff				controllers.SearchController.termVectorDiff
GET		/similarCollocations		controllers.SearchController.similarCollocations
GET		/dump						controllers.SearchController.dump