# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# An example controller showing a sample home page
GET     /search                     controllers.SearchController.search(q: Option[String], rf: Seq[String], n:Option[String], mf: Int ?= 1)
POST	/search						controllers.SearchController.search(q: Option[String], rf: Seq[String], n:Option[String], mf: Int ?= 1)
GET     /jsearch                    controllers.SearchController.jsearch(q: Option[String], rf: Seq[String], l:Int ?= 10, mf: Int ?= 1, p: Option[String])
POST	/jsearch					controllers.SearchController.jsearch(q: Option[String], rf: Seq[String], l:Int ?= 10, mf: Int ?= 1, p: Option[String])
GET		/terms						controllers.SearchController.terms(q: String,d:Int ?= 2,cp:Int ?= 0,t:Boolean ?= false)
GET		/collocations				controllers.SearchController.collocations(q: Option[String], ls: Int ?= 0, l: Int ?= 20, p: Option[String])
GET		/dump						controllers.SearchController.dump